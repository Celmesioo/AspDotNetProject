@model List<DataLogic.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div>
    <h4>ApplicationUser</h4>
    <hr />
    @foreach (var user in Model)
     {
        var namn = user.FirstName + ' ' + user.LastName;
        <div>
            <span><img src="@Url.Action("Image", "Home", new { id = user.Id} )" style="width:50px;height:50px; background: #FFFFFF;
    margin: auto;
    -moz-border-radius: 130px;
    border-radius: 40px;
    padding: 1px;
    box-shadow: 0px 0px 20px #888;" /></span>@Html.ActionLink(namn, "Details", "Account", new {id = user.Id }, null);
        </div>
     }
</div>
